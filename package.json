{
	"name": "@ctx-core/vanjs",
	"version": "4.0.109",
	"description": "ctx-core extensions for VanJS",
	"keywords": [
		"ctx-core",
		"mini-van-plate",
		"VanJS",
		"vanjs-core"
	],
	"homepage": "https://github.com/ctx-core/van#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/van/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/van.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./be_derive_pair",
		"./be_state_triple",
		"./H",
		"./props_clean",
		"./S",
		"./state",
		"./V_fragment",
		"./V_raw",
		"./van",
		"./vanx"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test:unit ###enable when https://github.com/vanjs-org/mini-van/issues/9 is fixed### && check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"@ctx-core/dom": "15.3.42",
		"ctx-core": "^6.5.1",
		"van-type-delegate": "1.2.15",
		"vanjs-ext": "^0.2.4"
	},
	"devDependencies": {
		"@types/jsdom": "^21.1.6",
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"jsdom": "^24.0.0",
		"mini-van-plate": "^0.5.4-perf.0",
		"tsx": "^4.7.1",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}