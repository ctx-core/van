{
	"name": "@ctx-core/vanjs",
	"version": "2.1.2",
	"description": "ctx-core extensions for VanJS",
	"keywords": [
		"ctx-core",
		"mini-van-plate",
		"VanJS",
		"vanjs-core"
	],
	"homepage": "https://github.com/ctx-core/van#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/van/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/van.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./src/index.d.ts",
	"svelte": "./src/index.js",
	"exports": {
		".": "./src/index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/array": "^26.15.7",
		"@ctx-core/dom": "15.0.3",
		"@ctx-core/object": "^29.0.0",
		"van-type-delegate": "1.1.1",
		"vanjs-ext": "^0.1.3"
	},
	"devDependencies": {
		"@types/jsdom": "^21.1.5",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"jsdom": "^22.1.0",
		"mini-van-plate": "^0.5.3",
		"tsx": "^3.14.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}